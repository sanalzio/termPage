{
    "name": "termPage",
    "version": "1.6.0",
    "author": "Sanalzio",
    "description": "My custom like terminal home page.",
    
    "manifest_version": 3,

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "terminal_settings": {

        "title": "New Tab",

        "tab_favicon": null,

        "user": "sanalzio",

        "host": null,

        "search-engine-url": "https://duckduckgo.com/?q=$&",

        "location": "Ankara",

        "language": "en",

        "allowLoadScript": false,

        "effectiveTime": false,

        "timeHours": 12,

        "remebmerHistory": false,

        "color_scheme": "dark"
    },

    "bookmarks": {
        "gh" : "https://github.com",
        "cd" : "https://discord.com",
        "yt" : "https://youtube.com",
        "insta" : "https://instagram.com",
        "linkedin" : "https://linkedin.com",
        "ws" : "https://web.whatsapp.com",
        "gmail" : "https://mail.google.com",
        "btt" : "https://btt.community"
    },

    "search_engines": {
        "yt": "https://www.youtube.com/results?search_query=$&",
        "dd": "https://devdocs.io/#q=$&",
        "g": "https://www.google.com/search?q=$&"
    },

    "aliases": {
        "cls": "clear",
        "?": "help",
        "wttr": "wttr.in",
        "reboot": "reload",
        "shutdown": "close",
        "js": "javascript",
        "lzar": "calc",
        "ip": "ipinfo",
        "ipconfig": "ipinfo"
    },

    "themes": [
        "./themes/glass.css",
        "./themes/custom-background.css"
    ]
}
